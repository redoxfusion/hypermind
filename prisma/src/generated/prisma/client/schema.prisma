generator client {
  provider = "prisma-client-js"
  output   = "src/generated/prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Word {
  id      Int      @id @default(autoincrement())
  answer  String
  image   String
  options String[]
  level   Int
}
